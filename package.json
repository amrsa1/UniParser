{
  "name": "uniparser",
  "version": "1.3.2",
  "description": "A universal parser for PDF, DOCX, TXT, MD, and HTML files into text",
  "main": "dist/cjs/uniparser.cjs.js",
  "module": "dist/esm/uniparser.esm.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/uniparser.esm.js",
      "require": "./dist/cjs/uniparser.cjs.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node index.js",
    "test": "mocha src/test",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amrsa1/uniparser.git"
  },
  "keywords": [
    "parser",
    "pdf",
    "docx",
    "txt",
    "html",
    "markdown",
    "file-parser",
    "text-extractor"
  ],
  "author": "Amr Salem",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0",
    "mammoth": "^1.8.0",
    "marked": "^14.1.3",
    "pdfjs-dist": "^4.7.76",
    "rollup-plugin-copy": "^3.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "chai": "^5.1.1",
    "mocha": "^10.7.3",
    "rollup": "^4.24.0"
  }
}
